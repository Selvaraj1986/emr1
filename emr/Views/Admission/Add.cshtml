                                                    <textarea asp-for="admissionsModel.directives_comments" class="form-control"></textarea>
                                                    <span asp-validation-for="@Model.admissionsModel.directives_comments" class="text-danger"></span>
                                                </div>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td><label class="control-label">Past Medical/Surgical Hx</label></td>
                                            <td>
                                                <div class="form-group">
                                                    <div class="checkbox">
                                                        <label for="AdmissionNoPastMed">
