                                                    <span asp-validation-for="@Model.admissionsModel.past_surg_history" class="text-danger"></span>
                                                </div>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td><label class="control-label">Chronic Conditions</label></td>
                                            <td>
                                                <div class="form-group">
                                                    <div class="checkbox">
                                                        <label for="AdmissionLungProblems">
                                                            <input asp-for="admissionsModel.lung_problems" type="checkbox" id="AdmissionLungproblems" />
                                                            <span asp-validation-for="@Model.admissionsModel.lung_problems" class="text-danger"></span> Lung
                                                            problems
                                                        </label>
                                                    </div>
                                                </div>
                                                <div id="admission_lung_problems_div" style="display: none;">
                                                    <div class="form-group">
                                                        <label for="lung_problems_description"
                                                               class="control-label">Lung Problems Description</label>
                                                        <textarea asp-for="admissionsModel.lung_problems_description" class="form-control"></textarea>
                                                        <span asp-validation-for="@Model.admissionsModel.lung_problems_description" class="text-danger"></span>
                                                    </div>
                                                </div>
                                                <script type="text/javascript">
                                                    $(function () {
                                                        enableAdmissionLungproblems($('#AdmissionLungproblems').is(':checked'));

                                                        $('#AdmissionLungproblems').change(function () {
                                                            enableAdmissionLungproblems($('#AdmissionLungproblems').is(':checked'));
                                                        });
                                                    });
                                                    function enableAdmissionLungproblems(enable) {
                                                        if (enable) {
                                                            $('#lung_problems_description').attr('required', 'required');
                                                            $('#admission_lung_problems_div').show();
                                                        } else {
                                                            $('#lung_problems_description').val('');
                                                            $('#lung_problems_description').removeAttr('required');
                                                            $('#admission_lung_problems_div').hide();
                                                        }
                                                    }
                                                </script>
