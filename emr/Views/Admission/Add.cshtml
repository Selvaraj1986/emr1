@model emr.Models.Model.PatientsModel
<div class="container-fluid">
    <div class="row" style="margin-top:20px;">
        <div class="breadcrumb">
            <a href="/emr/"><i class="fa fa-home fa-lg"></i> Home</a>/
            <a href="/Patients">Patients</a>/
            <a href="/Patients/view/@Model.id">@Model.first_name &nbsp @Model.last_name</a>/
            <a href="/Admissions">Admissions</a>

        </div>

        <h1>Add Admission</h1>
        <div class="row-fluid">
            <form asp-action="Add">
                <div class="col-md-8">

                    <div role="tabpanel">
                        <!-- Nav tabs -->
                        <ul class="nav nav-tabs" role="tablist">
                            <li role="presentation" class="active">
                                <a href="#home" aria-controls="home" role="tab"
                                   data-toggle="tab">Admission</a>
                            </li>
                        </ul>

                        <!-- Tab panes -->
                        <div class="tab-content">
                            <div role="tabpanel" class="tab-pane active" id="home">

                                <table class="table table-striped" style="width: 95%">
                                    <tbody>
                                        <tr>
                                            <td style="width: 20%"><label class="control-label">Area</label></td>
                                            <td style="width: 80%"><label class="control-label">Admission</label></td>
                                        </tr>
                                        <tr>
                                            <td><label class="control-label">Record Date</label></td>
                                            <td>
                                                <div class="form-group">
                                                    <input type="hidden" asp-for="admissionsModel.id" />
                                                    <input asp-for="admissionsModel.record_date" class="form-control datepicker" type="text" id="datepicker" />
                                                    <span asp-validation-for="@Model.admissionsModel.record_date" class="text-danger"></span>
                                                </div>
                                                <div class="form-group">
                                                    <label for="AdmissionComplaint" class="control-label">
                                                        Reason for
                                                        Admission (Pt’s Own Words)
                                                    </label>
                                                    <textarea asp-for="admissionsModel.complaint" class="form-control"></textarea>
                                                    <span asp-validation-for="@Model.admissionsModel.complaint" class="text-danger"></span>
                                                </div>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td><label class="control-label">Admission Data</label></td>
                                            <td>
                                                <div class="form-group">
                                                    <label for="AdmissionInformant"
                                                           class="control-label">Informant</label>
                                                    <select id="admissionsModel.informant" class="form-control col-md-8" name="admissionsModel.informant"
                                                            asp-for="@Model.admissionsModel.informant">
                                                        <option value="">-- Choose --</option>
                                                        <option value="patient">patient</option>
                                                        <option value="other">other</option>
                                                    </select>
                                                </div>
                                                <div class="form-group">
                                                    <label for="AdmissionInterpretive" class="control-label">
                                                        Does
                                                        patient require interpretive services? If so make appropriate
                                                        selections.
                                                    </label>
                                                    <select id="admissionsModel.interpretive" class="form-control col-md-8" name="admissionsModel.interpretive"
                                                            asp-for="@Model.admissionsModel.interpretive">
                                                        <option value="">-- Choose --</option>
                                                        <option value="No">No</option>
                                                        <option value="Sign language interpreter">
                                                            Sign language
                                                            interpreter
                                                        </option>
                                                        <option value="Foreign language interpreter-specify in comments">
                                                            Foreign language interpreter-specify in comments
                                                        </option>
                                                        <option value="Other (Specify in comments">
                                                            Other (Specify in
                                                            comments
                                                        </option>
                                                    </select>
                                                </div>
                                                <div class="form-group">
                                                    <label for="AdmissionAdmittedVia" class="control-label">
                                                        Admitted
                                                        Via
                                                    </label>
                                                    <select id="admissionsModel.admitted_via" class="form-control col-md-8" name="admissionsModel.admitted_via"
                                                            asp-for="@Model.admissionsModel.admitted_via">
                                                        <option value="">-- Choose --</option>
                                                        <option value="Ambulatory">Ambulatory</option>
                                                        <option value="Ambulance">Ambulance</option>
                                                        <option value="Stretcher">Stretcher</option>
                                                        <option value="Wheelchair">Wheelchair</option>
                                                        <option value="Parents arms">Parents arms</option>
                                                    </select>
                                                </div>
                                                <div class="form-group">
                                                    <label for="AdmissionAdmittedFrom" class="control-label">
                                                        Admitted
                                                        From
                                                    </label>
                                                    <select id="admissionsModel.admitted_from" class="form-control col-md-8" name="admissionsModel.admitted_from"
                                                            asp-for="@Model.admissionsModel.admitted_from">
                                                        <option value="">-- Choose --</option>
                                                        <option value="ER">ER</option>
                                                        <option value="Home">Home</option>
                                                        <option value="Surgery">Surgery</option>
                                                        <option value="Physician office">Physician office</option>
                                                        <option value="Nursing home">Nursing home</option>
                                                        <option value="Hospital transfer">Hospital transfer</option>
                                                    </select>
                                                </div>
                                                <div class="form-group">
                                                    <label for="AdmissionContact"
                                                           class="control-label">Contact Person</label>
                                                    <input asp-for="admissionsModel.contact" class="form-control" />
                                                    <span asp-validation-for="@Model.admissionsModel.contact" class="text-danger"></span>
                                                </div>
                                                <div class="form-group">
                                                    <label for="AdmissionOrientation"
                                                           class="control-label">Orientation</label>

                                                    <select id="admissionsModel.orientation" multiple class="form-control col-md-8" name="admissionsModel.orientation"
                                                            asp-for="@Model.admissionsModel.orientation">
                                                        <option value="">-- Multiple Choice --</option>
                                                        <option value="No smoking policy">No smoking policy</option>
                                                        <option value="intercom/bed controls">
                                                            intercom/bed controls
                                                        </option>
                                                        <option value="Call light/beside and bathroom">
                                                            Call light/beside
                                                            and bathroom
                                                        </option>
                                                        <option value="Phone/TV">Phone/TV</option>
                                                        <option value="Visiting hours">Visiting hours</option>
                                                        <option value="Patient consent for use of siderails">
                                                            Patient
                                                            consent for use of siderails
                                                        </option>
                                                    </select>
                                                </div>
                                                <div class="form-group">
                                                    <label for="AdmissionBelongings" class="control-label">
                                                        Belongings
                                                        accompanying patient
                                                    </label>
                                                    <select id="admissionsModel.belongings" class="form-control col-md-8" name="admissionsModel.belongings"
                                                            asp-for="@Model.admissionsModel.belongings">
                                                        <option value="">-- Multiple Choice --</option>
                                                        <option value="Glasses">Glasses</option>
                                                        <option value="L contact">admission_L contact</option>
                                                        <option value="R contact">R contact</option>
                                                        <option value="Eye prothesis">Eye prothesis</option>
                                                        <option value="Dentures/complete upper">
                                                            Dentures/complete upper
                                                        </option>
                                                        <option value="Dentures/complete lower">
                                                            Dentures/complete lower
                                                        </option>
                                                        <option value="Dentures/partial upper">
                                                            Dentures/partial upper
                                                        </option>
                                                        <option value="Dentures/partial lower">
                                                            Dentures/partial lower
                                                        </option>
                                                        <option value="L hearing aid">L hearing aid</option>
                                                        <option value="R hearing aid">R hearing aid</option>
                                                        <option value="Prosthetic devices">Prosthetic devices</option>
                                                        <option value="None">None</option>
                                                    </select>
                                                </div>
                                                <div class="form-group">
                                                    <label for="AdmissionValuablesDisposition"
                                                           class="control-label">Valuables Disposition</label>
                                                    <select id="admissionsModel.valuables_disposition" class="form-control col-md-8" name="admissionsModel.valuables_disposition"
                                                            asp-for="@Model.admissionsModel.valuables_disposition">
                                                        <option value="">-- Choose --</option>
                                                        <option value="admission_Taken home">
                                                            admission_Taken home
                                                        </option>
                                                        <option value="Secured by hospital">Secured by hospital</option>
                                                        <option value="None">None</option>
                                                    </select>
                                                </div>
                                                <div class="form-group">
                                                    <label for="AdmissionValuablesComments"
