                                                            <span asp-validation-for="@Model.admissionsModel.organ_donor" class="text-danger"></span> Organ Donor
                                                        </label>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="AdmissionDirectivesComments"
