                                                            <span asp-validation-for="@Model.admissionsModel.others_past_med" class="text-danger"></span> Others
                                                        </label>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="AdmissionPastComments"
                                                           class="control-label">Past Medical Comments</label>
                                                    <textarea asp-for="admissionsModel.past_comments" class="form-control"></textarea>
                                                    <span asp-validation-for="@Model.admissionsModel.past_comments" class="text-danger"></span>
                                                </div>
                                                <div class="form-group">
