                                                <div class="form-group">
                                                    <div class="checkbox">
                                                        <label for="AdmissionLivingWill">
                                                            <input asp-for="admissionsModel.living_will" type="checkbox" />
                                                            <span asp-validation-for="@Model.admissionsModel.living_will" class="text-danger"></span>Living Will
                                                        </label>
                                                    </div>
