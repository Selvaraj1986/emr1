@model emr.Models.Model.PatientsModel
<div class="container-fluid">
    <div class="row" style="margin-top:20px;">
        <div class="breadcrumb">
            <a href="/emr/"><i class="fa fa-home fa-lg"></i> Home</a>/<a href="/Patients">Patients</a>/<a href="/Patients/view/@Model.id">@Model.first_name &nbsp @Model.last_name</a>
        </div>


        <div class="alert alert-success alert-dismissible" role="alert" style="display: none;">
            <button type="button" class="close" data-dismiss="alert">
                <span aria-hidden="true">×</span>
                <span class="sr-only">Close</span>
            </button>
            The note has been saved.
        </div>
        <partial name="_PatientsInfo" model="@Model" />

        <div class="command-box">
            <a href="/Patients/edit/@Model.id" class="btn btn-success btn-sm">Edit Details</a>
            <a href="/Patients/delete/@Model.id" class="btn btn-danger btn-sm" onclick="if (confirm(&quot;Are you sure to delete this record?&quot;)) { document.post_637394e96714c872783716.submit(); } event.returnValue = false; return false;">Delete Patient</a>
            <a href="#" class="btn btn-danger btn-sm" onclick="if (confirm(&quot;Are you sure to reset this record?&quot;)) { document.post_637394e96724c720524430.submit(); } event.returnValue = false; return false;">
                Delete Student Inputs
            </a>
            <a href="#" class="btn btn-danger btn-sm" onclick="if (confirm(&quot;Are you sure to reset this record?&quot;)) { document.post_637394e96731e817266781  t(); } event.returnValue = false; return false;">
                Delete Instructor Inputs
            </a>
        </div>

        <div role="tabpanel">
            <!-- Nav tabs -->
            <partial name="_PatientTabs" />
            <!-- Tab panes -->
            <div class="tab-content">
                <div role="tabpanel" class="tab-pane active" id="home">
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Value</th>
                            </tr>
                        </thead>

                        <tbody>
                            <tr>
                                <td>Id</td>
                                <td>@Model.id</td>
                            </tr>
                            <tr>
                                <td>Medical Record #</td>
                                <td>@Model.medical_number</td>
                            </tr>
                            <tr>
                                <td>LastName</td>
                                <td>@Model.last_name;</td>
                            </tr>
                            <tr>
                                <td>FirstName</td>
                                <td>@Model.first_name</td>
                            </tr>
                            <tr>
                                <td>DOB</td>
                                <td>@Model.dob</td>
                            </tr>
                            <tr>
                                <td>Gender</td>
                                <td>@Model.gender</td>
                            </tr>
                            <tr>
                                <td>Provider</td>
                                <td>@Model.provider</td>
                            </tr>
                            <tr>
                                <td>Description</td>
                                <td>
                                    @Model.description
                                </td>
                            </tr>
                            <tr>
                                <td>Creator</td>
                                <td>
                                    @Model.creator
                                </td>
                            </tr>
                            <tr>
                                <td>Created</td>
                                <td>
                                    @Model.created
                                </td>
                            </tr>

                            <tr>
                                <td>Modifier</td>
                                <td>
                                    @Model.modifier
                                </td>
                            </tr>
                            <tr>
                                <td>Modified</td>
                                <td>
                                    @Model.modified
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
